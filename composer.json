{
  "name": "zitro",
  "license": "MIT",
  "type": "project",
  "description": "Zitro Test Login",
  "require": {
    "php": ">7.4.2.*",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/yaml": "^4.3",

    "lambdish/phunctional": "^1.0",
